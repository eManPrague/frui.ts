{
  "name": "frui.ts",
  "version": "0.1.0",
  "description": "MVVM UI framework",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run build:framework && npm run build:demo && npm run build:storybook && npm run build:docs",
    "build:framework": "webpack --config-name framework",
    "build:demo": "webpack --config-name demo",
    "build:storybook": "build-storybook -c .storybook -o dist/storybook",
    "build:docs": "typedoc src --out dist/docs --hideGenerator",
    "test": "jest",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "validate": "tsc --noEmit && npm run lint",
    "lint": "tslint --project .",
    "demo": "webpack-dev-server --config-name demo",
    "demoproxy": "lcp --proxyUrl https://redmine.dev.eman.cz"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.eman.cz:frui.ts/frui.ts-fe.git"
  },
  "keywords": [
    "mvvm",
    "react",
    "mobx"
  ],
  "author": "Augustin Å ulc",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-actions": "^5.0.5",
    "@storybook/addon-info": "^5.0.5",
    "@storybook/addon-storysource": "^5.0.5",
    "@storybook/react": "^5.0.5",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__react": "^4.0.1",
    "@types/typescript": "^2.0.0",
    "fetch-with-proxy": "^1.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.5.0",
    "local-cors-proxy": "^1.0.2",
    "react-docgen-typescript-loader": "^3.0.1",
    "ts-jest": "^24.0.1",
    "ts-loader": "^5.3.3",
    "tslint": "^5.14.0",
    "tslint-react": "^4.0.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-no-inherit": "^1.1.5",
    "typescript": "^3.4.1",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "bind-decorator": "^1.0.11",
    "mobx": "^4.9.4",
    "mobx-react-lite": "^1.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
