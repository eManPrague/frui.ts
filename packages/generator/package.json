{
  "name": "@frui.ts/generator",
  "publishConfig": {
    "access": "public"
  },
  "version": "999.0.0",
  "description": "Frui.ts code generator",
  "keywords": [
    "code-generator"
  ],
  "homepage": "https://github.com/eManPrague/frui.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eManPrague/frui.ts.git"
  },
  "bugs": {
    "url": "https://github.com/eManPrague/frui.ts/issues"
  },
  "author": "Augustin Å ulc <sulc.gusta@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/generator.umd.js",
  "exports": {
    ".": {
      "require": "./dist/generator.umd.js"
    }
  },
  "bin": {
    "fruits-generate": "./dist/generator.umd.js"
  },
  "scripts": {
    "start-openapi": "ts-node src/index.ts openapi -o temp/apitest",
    "clean": "rimraf dist",
    "build": "tsc && vite build",
    "postbuild": "shx mkdir dist/openapi && shx cp -r src/openapi/templates dist/openapi"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "cli-progress": "^3.12.0",
    "commander": "^10.0.1",
    "handlebars": "^4.7.7",
    "lodash": "^4.17.21",
    "openapi-types": "^12.1.3",
    "ts-morph": "^18.0.0"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.0",
    "@types/lodash": "4.14.195",
    "@types/node": "20.3.0",
    "nodemon": "2.0.22",
    "rimraf": "^5.0.1",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "vite-plugin-banner": "^0.7.0"
  }
}
